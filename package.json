{
    "name": "ulid-workers",
    "version": "0.1.0",
    "description": "ULID generator for Cloudflare workers",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "npm run clean && tsc",
        "clean": "rimraf dist",
        "format": "prettier --write \"{source,test}/**/*.{js,ts}\"",
        "prepublishOnly": "npm run build",
        "test": "npm run build && npm run test:specs && npm run test:format",
        "test:format": "prettier --check \"{source,test}/**/*.{js,ts}\"",
        "test:specs": "nyc mocha -t 10000 'test/**/*.spec.js'"
    },
    "files": [
        "dist/**/*"
    ],
    "lint-staged": {
        "{source,test}/**/*.{js,ts}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ryan-mars/ulid-workers.git"
    },
    "keywords": [
        "ulid",
        "uuid",
        "id",
        "generator",
        "guid"
    ],
    "author": "Ryan Marsh <ryan@stochastic.dev>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ryan-mars/ulid-workers/issues"
    },
    "homepage": "https://github.com/ryan-mars/ulid-workers#readme",
    "devDependencies": {
        "@types/node": "^15.12.0",
        "chai": "^4.3.4",
        "husky": "^4.3.8",
        "lint-staged": "^11.0.0",
        "mocha": "^8.4.0",
        "nyc": "^15.1.0",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.2",
        "sinon": "^11.1.1",
        "typescript": "^4.3.2"
    },
    "dependencies": {}
}
